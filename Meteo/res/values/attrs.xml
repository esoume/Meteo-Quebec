<?xml version="1.0" encoding="utf-8"?>
<resources>

	<declare-styleable
		name="com.techsolcom.android.android.utils.HorizontalPager">
		<attr name="pageWidth" format="dimension" />
	</declare-styleable>

	<declare-styleable name="com.techsolcom.android.widgets.PagerControl">
		<attr name="barColor" format="color" />
		<attr name="highlightColor" format="color" />
		<attr name="fadeDelay" format="integer" />
		<attr name="fadeDuration" format="integer" />
		<attr name="roundRectRadius" format="dimension" />
	</declare-styleable>

	<declare-styleable name="com.techsolcom.android.utils.TouchListView">
		<attr name="normal_height" format="dimension" />
		<attr name="expanded_height" format="dimension" />
		<attr name="grabber" format="reference" />
		<attr name="dragndrop_background" format="color" />
		<attr name="remove_mode">
			<enum name="none" value="-1" />
			<enum name="fling" value="0" />
			<enum name="slide" value="1" />
			<enum name="slideRight" value="1" />
			<enum name="slideLeft" value="2" />
		</attr>
	</declare-styleable>

	<declare-styleable name="com.techsolcom.android.utils.SwipeyTabs">
		<attr name="bottomBarColor" format="color" />
		<attr name="bottomBarHeight" format="dimension" />
		<attr name="tabIndicatorHeight" format="dimension" />
	</declare-styleable>

	<declare-styleable name="com.techsolcom.android.widgets.DashBoardButtton">
		<attr name="text" format="string" />
		<attr name="textColor" format="color" />
		<attr name="textSize" format="dimension" />
	</declare-styleable>
</resources>